            <string>cursor_in_area = point_in_rect(mouse_x,mouse_y,xmin,ymin,xmax,ymax)

if cursor_in_area {
    xsnapped = floor(mouse_x/snap)*snap
    ysnapped = floor(mouse_y/snap)*snap
    x = mouse_x
    y = mouse_y
    
    // creating
    if mouse_check_button_pressed(mb_left) and global.state == globalstate_idle {
        with all if object_is_edit(object_index) undo_recent = false
        undo_objectstring = 'create'
        
        var i = instance_create(xsnapped,ysnapped,selected_object);
        i.undo_recent = true
    }
    // destroying
    else if mouse_check_button_pressed(mb_right) {
        with all if object_is_edit(object_index) undo_recent = false
        undo_objectstring = ''
        
        with all {
            if object_is_edit(object_index) and place_meeting(x,y,oEdit) {
                other.undo_objectstring += edit_object_serialize(self)
                instance_destroy()
            }
        }
    }
}

// undo
if keyboard_check(vk_control) and keyboard_check_pressed(ord('Z')) and undo_objectstring != '' {
    if undo_objectstring == 'create' {
        with all {
            if object_is_edit(object_index) {
                if undo_recent instance_destroy()
            }
        }
    }
    else {
        create_from_serialization(undo_objectstring)
    }
}
